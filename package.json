{
  "name": "svelte-restate",
  "description": "Immutable store for Svelte with full Typescript support and Redux Devtools integration",
  "version": "0.1.1",
  "author": "Stepan Lusnikov <endenwer@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/endenwer/svelte-restate.git",
  "main": "dist/index",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "svelte",
    "store"
  ],
  "files": [
    "dist",
    "devtools",
    "README.MD"
  ],
  "scripts": {
    "docs": "docsify serve",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build",
    "test": "mocha --require sucrase/register --recursive src/**/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "immer": "^9.0.1"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "docsify-cli": "^4.4.3",
    "immer": "^9.0.1",
    "mocha": "^8.3.2",
    "rollup": "^2.44.0",
    "rollup-plugin-dts": "^3.0.1",
    "sucrase": "^3.17.1",
    "svelte": "^3.37.0",
    "typescript": "^4.2.3"
  }
}
